     function getInternetExplorerVersion_2780() {
         // Returns the version of Internet Explorer or a -1
         // (indicating the use of another browser).

             var rv = -1; // Return value assumes failure.
             if (navigator.appName == 'Microsoft Internet Explorer') {
                 isIEBrowser_2780=true;
                 var ua = navigator.userAgent;
                 var re  = new RegExp("MSIE ([0-9]{1,}[\.0-9]{0,})");

                 if (re.exec(ua) != null)
                     rv = parseFloat( RegExp.$1 );
             }

             return rv;
         }

      //returns true if ie version is less than 9, say ie6, ie7, ie8
         // -1 for non IE browsers.
         function isIEBrowserWithVersionLessThan9_2780 () {

             browserVersion_2780 = getInternetExplorerVersion_2780();  //-1 for non IE browsers
             if((browserVersion_2780 != -1) && (browserVersion_2780 < 9)) {
                 return true;

             }
             return false;
         }

    //code to detect Edge Features, courtesy  (http://dl.dropboxusercontent.com/u/13483458/test-edge.html)
    var testEle_2780=document.createElement("div_2780");
    function isSupported_2780(a){

        var d=testEle_2780.style,e;
        for(i=0;i<a.length;i++)
            if(e=a[i],d[e]!==void 0)
                return!0;
        return!1
    }

    function supportsRGBA_2780(){

        testEle_2780.cssText="background-color:rgba(150,255,150,.5)";
        if((""+testEle_2780.style.backgroundColor).indexOf("rgba")==0)
            return!0;
        return!1
    }

    var hasTransform_2780=isSupported_2780([
        "transformProperty",
        "WebkitTransform",
        "MozTransform",
        "OTransform",
        "msTransform"
    ]),

    hasSVG_2780=!!document.createElementNS&&!!document.createElementNS("http://www.w3.org/2000/svg","svg").createSVGRect,
    hasRGBA_2780=supportsRGBA_2780(),
    hasJSON_2780=window.JSON&&window.JSON.parse&&window.JSON.stringify,
    readyToPlay=!1;

    function isIEBrowserVersion9_2780() {
        return (isIEBrowser_2780 && (browserVersion_2780 == 9)) ? true : false;
    }

    function isEdgeSupported_2780() {
        if(isIEBrowserVersion9_2780()) {
            return "y";           //hardcoding IE9 edge support.
        }
        if(hasTransform_2780) {
            if(requiresSVG_2780&&!hasSVG_2780)
                return "f";
            return "y";
        }
        return "f";
    }

    function isCanvasSupported_2780(){
      var elem = document.createElement('canvas');
      return !!(elem.getContext && elem.getContext('2d'));
    }

    function isHTML5FeaturesSupported_2780() {
         return (isCanvasSupported_2780()) ? "y" : "f";
    }

    var requiresSVG_2780=false;
    //edge detection code end

